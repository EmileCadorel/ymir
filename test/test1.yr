struct (a : int) A;


def print (s : string) {
    let i = 0;
    while (i < s.length) {
	system (print_c, s[i]);
	i++;
    }
}

def print (c : char) {
    system (print_c, c);
}

def print (i : int) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

def println (i) {
    print (i);
    print ('\n');
}

def print (a : A) {
    print ('A(');
    print (a.a);
    print (')');
}

def print (a : array!A) {
    let i = 0;
    print ('[');
    while (i < a.length) {
	print (a [i]);
	if (i < a.length - 1) print (', ');
	i ++;
    }
    print (']');
}


def main () {
    let test = [A (0), A (1)];
    test += [A (90)];
    println (test);
}


