import std.stdio;
import std.string;

struct
| a : int
-> Test;

def opBinary (op : string, A) (a : A, b : A) {
    if (op.equals ("+")) {
	return Test (a.a + b.a);
    }
    return null;
}

def main () {
    let a = Test (10), b = Test (12);
    let c = a.opBinary!("+")(b).a;
    println ("Test (%d)", c);
}
