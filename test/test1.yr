    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

def print (i : long) : void {
    if (i < 0) {
	system (print_c, '-');
	let mi = -i;
	if (mi < 0) print ("inf");
	else print (mi);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}


