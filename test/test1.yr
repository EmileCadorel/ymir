
extern (C) putchar (c : char);

def print (s : string) {
    let i = 0;
    while (i < s.length) {
	putchar (s [i]);
	i ++;
    }
}

def print (c : char) {
    putchar (c);
}

def println (a) {
    print (a);
    print ('\n');
}

