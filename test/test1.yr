struct (a : int) A;


def println (s : string) {
    let i = 0;
    while (i < s.length) {
	system (print_c, s[i]);
	i++;
    }
    system (print_c, '\n');
}

def print (c : char) {
    system (print_c, c);
}

def print (i : int) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

def println (i) {
    print (i);
    print ('\n');
}


def test (a : ref!A) {
    a = A (2345);
}

def main () {
    let root = A (0);
    test (root);
    println (root.a);
}


