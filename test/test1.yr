struct (i : int) A;

def print (i : int) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

def print (s : string) {
    for it in s
	print (it);
}

def print (c : char) {
    system (print_c, c);
}

def test (a : [char]) {
    for it in a
	       print (it);
}


def main () {
    let a = cast:[char] ("salut");
    test (a);
    let b = [char];
    print (b[0]);
    return 0;
}


