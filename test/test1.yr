struct (i : int) A;

def print (c : char) {
    system (print_c, c);
}

def print (i : int) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

def main () {
    for (it in 0 .. -10) {
	print (it);
	print ('\n');
    }
}


