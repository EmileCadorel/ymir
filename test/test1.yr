import std.stdio;
import std.array;

def foo (T) (a : [int]) {
    println ('fst');
    let sum = T.init;
    for (it in a) {
	sum += it;
    }
    return sum;
}

def foo (a : [ulong]) : ulong {
    println ('second');
    return 10ul;
}


def main () {
    println (foo ([1, 2, 3]));
    println (foo ([1ul, 2ul]));
}
