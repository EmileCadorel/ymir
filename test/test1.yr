import std.stdio;
import std.string;

struct
| a : int
-> Test;

def opBinary (op : string) (a : Test, b : Test) {
    if (op.equals ("+")) {
	return Test (a.a + b.b);
    }
    return null;
}

def print (A) (a : A) {
    print (A.typeid, '(', a.a, ')');
}




def main () {
    let a = Test (10), b = Test (12);
    println (a.opBinary!"+" (b));
}
