def printIntNeg (i : int) : void {
    if (i < -9) print (i / 10);
    system (print_c, (i % 10) + '0');
}

def print (i : int) : void {
    if (i < 0) {
	let j = -i;
	system (print_c, '-');
	printIntNeg (j);	
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

