def print (s : string) for it in s print (it);
def toStr (c) return cast:string ([c]);

def exp (x, y)  {
    let f = 1l;
    while (y > 0) {
	if (y % 2 == 0) {
	    f *= f;
	} else f = f * f * x;
	y = y / 2;
    }
    return f;
}

def print (a : float, prec : int) {
    let i = cast:long (a * (exp (10, prec)));
    let ret = "", inf = false;
    if (i < 0) {
	inf = true;
	i = -i;
    }

    while (i > 9) {
	ret = toStr((i%10) + '0') + ret;
	i /= 10;
	prec --;
	if (prec == 0) ret = "." + ret;
    }

    ret = toStr((i%10) + '0') + ret;
    if (prec >= 1) ret = "." + ret;

    if (inf) print( "-" + ret);
    else print (ret);    
}

