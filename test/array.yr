
extern (C) putchar (a : char);

def print (str : string) {
    let i = 0;
    while (i < str.length) {
	print (str [i]);
	i += 1;
    }    
}

def print (c : char) {
    putchar (c);
}

def print (i : int) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

def println (s) {
    print (s);
    print ('\n');
}

def print (arr : array!int) {
    let i = 0;
    print ('[');
    while (i < arr.length) {
	print (arr [i]);
	if (i < arr.length - 1) print(", ");
	i += 1;
    }
    print (']');
}

