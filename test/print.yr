extern (C) putchar (c : char);

def print (c : char) {
    putchar (c);
}

def println (a : array!int) {
    let i = 0;
    print ('[');
    while (i < a.length) {
	print (a[i]);
	if (i < a.length - 1) print (',');
	i++;
    }
    print(']');
    print ('\n');
}

