import std.math;
import std.algorithm.sorting;

def opBinary ('+') (a : string, b : char) {
    return a + ([b]).to!string;
}

def opUnary ('++') (a : ref!string) {
    a = a + "++";
    return a;
}


def main () {
    let a = "salut";
    let b = (a++) + 'y';
    println (b, " ", a);
}


