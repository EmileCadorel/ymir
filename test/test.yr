def suite (T) (a : [T], ind : int) : [T] {
    return [a [ind]];
}

def suite (T) (a : [T], ind : int, left) : [T] {
    return [a [ind]] + a.suite!(T) (expand (left));
}   

def opIndex (T) (a : [T], ind : int, left : int) : [T] {
    return [a [ind], a [left]];
}

def opIndex (T) (a : [T], ind : int, left) : [T] {
    return [a [ind]] + a [expand (left)];
}


