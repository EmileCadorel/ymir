import std.stdio;
import std.conv;
import std.math, std.array;

def binaryFun (op : string, T) () {
    mixin (	   
	   "let ret = (a : "  + T.typeid + ", b : " + T.typeid + ") =>" + op + ";" +
	   "return ret; "
    );
}


def main () {    
    let b = 10;
    println (binaryFun!(({ a - b * a }), int) () (10, 12));
}




		 
