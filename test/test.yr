def opTest ('>') (a : string, b : string) {
    for (it in 0UL .. min (a.length, b.length)) {
	if (a [it] < b [it]) return false;
	else if (a [it] > b [it]) return true;
    }
    return false;
}    

def opTest ('<') (a : string, b : string) {
    for (it in 0UL .. min (a.length, b.length)) {
	if (a [it] > b [it]) return false;
	else if (a [it] < b [it]) return true;
    }
    return false;
}    

def main () {
    let b = 'salut' < 'salut';
