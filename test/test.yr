import std.stdio;

struct
| a : int
-> Test;


def opBinaryRight (op : string) (a : Test, b : Test) {
    static if (op == "+") 
	return a.a + b.a;
    else if (op == "-") {
	static assert (false, "ici");
    } else {
	return Test (a.a * b.a);
    }
}

def main () {
    println (Test(2) + Test (2) * Test (8));
}


