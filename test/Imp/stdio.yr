/**
   Affiche un int.   
 */
def print (i : long) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

/**
   Affiche un long
 */
