	system (print_c, (i%10) + '0');
    }
}

def println (s) {
    print (s);
    print ('\n');
}

def print (arr) {
    let i = 0;
    print ('[');
    while (i < arr.length) {
	print (arr [i]);
	if (i < arr.length - 1) print(", ");
	i += 1;
    }
    print (']');
}

