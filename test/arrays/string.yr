def toStr (i : int) : string {
    let ret = "", inf = false;
    if (i < 0) {
	inf = true;
	i = -i;
    }

    while (i > 9) {
	ret = toStr((i%10) + '0') + ret;
	i /= 10;
    }

    ret = toStr((i%10) + '0') + ret;

    if (inf) return "-" + ret;        
    return ret;
}

def toStrReq (i : int) : string {
    if (i < 0) {
	return "-" + toStrReq (-i);
    } else {
	let ret = '';
	if (i > 9) ret = toStrReq (i / 10);
	return ret + toStr ((i%10) + '0');
    }
}


def print (i : int) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
