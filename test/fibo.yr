def print (i : int) : void {
    if (i < 0) {
	system (print_c, '-');
	print (- i);
    } else {
	if (i > 9) print (i / 10);
	system (print_c, (i%10) + '0');
    }
}

def fibo (n : int) {
    if (n < 2) return n;
    else {
	let sum = fibo (n - 1) + fibo (n - 2);
	return sum;
    }
}

def main () {
    let a = fibo (12);
    print (a);
}
